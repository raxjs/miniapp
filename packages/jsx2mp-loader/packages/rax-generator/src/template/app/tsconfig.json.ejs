<%_ const useReact = projectFeatures.includes('react') -%>
{
  "compilerOptions": {
    "module": "esNext",
    "target": "es2015",
    "outDir": "build",
    "jsx": "preserve",
    "jsxFactory": "createElement",
    "moduleResolution": "node",
    "sourceMap": true,
    "alwaysStrict": true,
    "baseUrl": "."<%_ if (useReact) { -%>,
    "paths": {
      "react": [
        "node_modules/@rax-types/rax"
      ]
    }<%_ } -%>
    
  },
  "include": [
    "./src/**/*"
  ]
}