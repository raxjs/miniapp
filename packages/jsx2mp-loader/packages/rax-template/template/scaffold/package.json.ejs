{
  "name": "<%= npmName %>",
  "author": "<%= projectAuthor %>",
  "scripts": {
    "build": "build-scripts build",
    "start": "build-scripts start"
  },
  "dependencies": {
    <%_ if (projectFeatures.includes('ssr')) { -%>
    "rax-server-renderer": "^1.1.0",
    <%_ } -%>
    "rax": "^1.0.8",
    <%_ if (scaffoldType === 'standard') { -%>
    "rax-app": "^0.2.0",
    <%_ } -%>
    <%_ if (scaffoldType === 'lite' || scaffoldType === 'mpa-standard') { -%>
    "driver-universal": "^2.0.0",
    <%_ } -%>
    "rax-image": "^1.0.3",
    "rax-link": "^1.0.1",
    "rax-text": "^1.0.0",
    "rax-view": "^1.0.0"
  },
  "devDependencies": {
    <%_ if (projectFeatures.includes('ssr')) { -%>
    "build-plugin-rax-ssr": "^0.1.0",
    <%_ } -%>
    <%_ if (projectFeatures.includes('serverless')) { -%>
    "build-plugin-rax-faas": "^0.1.0",
    <%_ } -%>
    <%_ if (scaffoldType === 'mpa-standard') { -%>
    "build-plugin-rax-multi-pages": "^0.1.2",
    <%_ } %>
    "build-plugin-rax-app": "^0.1.0",
    "@alib/build-scripts": "^0.1.0"
  }
